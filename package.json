{
  "name": "structurae",
  "version": "0.0.16",
  "description": "Data structures for performance-sensitive modern JavaScript applications.",
  "main": "index.js",
  "keywords": [
    "optimization",
    "binary",
    "data structures",
    "sorted",
    "array",
    "bitfield",
    "bigint"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint ./lib/*.js ./test/*.js",
    "coverage:report": "cat ./coverage/lcov.info | codecov",
    "doc:api": "jsdoc2md > doc/API.md",
    "benchmark": "node benchmark.js"
  },
  "directories": {
    "lib": "lib",
    "doc": "doc",
    "test": "test"
  },
  "author": "Maga D. Zandaqo <denelxan@gmail.com> (http://maga.name)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zandaqo/structurae.git"
  },
  "homepage": "https://github.com/zandaqo/structurae#readme",
  "bugs": {
    "url": "https://github.com/zandaqo/structurae/issues"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "benchmark": "^2.1.4",
    "codecov": "^3.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.0.0",
    "jsdoc-to-markdown": "^4.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/lib/**",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
  "engines": {
    "node": ">=10.4.0"
  }
}
